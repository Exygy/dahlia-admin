    "@babel/preset-env": "^7.4",
    "@babel/preset-react": "^7.0",
    "@babel/runtime": "^7.4",
    "@rails/webpacker": "^4.0.2",
    "axios": "^0.17.0",
    "babel-plugin-root-import": "^6.2.0",
    "classnames": "^2.2.5",
    "coffeescript": "1.12.7",
    "final-form": "^4.12.0",
    "grunt": "0.4.5",
    "grunt-contrib-clean": "0.6.0",
    "grunt-contrib-copy": "0.8.1",
    "grunt-json-remove-fields": "^0.1.2",
    "grunt-replace": "0.11.0",
    "grunt-sort-json": "0.0.5",
    "lodash": "^4.17.4",
    "moment": "^2.19.1",
    "postcss-smart-import": "^0.7.6",
    "prop-types": "^15.6.0",
    "puppeteer": "^1.7.0",
    "react": "^16.8.6",
    "react-datepicker": "^2.4.0",
    "react-dom": "^16.8.6",
    "react-dropzone": "^10.1.3",
    "react-final-form": "^4.1.0",
    "react-form": "2.6.1",
    "react-json-tree": "^0.11.0",
    "react-modal": "^3.4.4",
    "react-scrollable-anchor": "^0.6.1",
    "react-table": "^6.7.4",
    "turbolinks": "^5.0.3",
    "webpack": "^4.5",
    "webpack-cli": "^3.3.2",
    "webpacker-react": "^0.3.2"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "caniuse-lite": "^1.0.30000760",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^5.4.0",
    "eslint-plugin-class-property": "1.0.6",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "7.4.0",
    "jest": "^24.8",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^6.6.0",
    "react-test-renderer": "^16.3.2",
    "sinon": "^4.5.0",
    "snazzy": "^7.1.1",
    "standard": "^11.0.1",
    "webpack-dev-server": "^3.3.1"
  },
  "engines": {
    "node": "12.1.0",
    "npm": "6.9.0",
    "yarn": "1.x.x"
  },
  "scripts": {
    "lint:fix": "standard \"./**/*.js\" --fix | snazzy",
    "lint": "standard \"./**/*.js\" | snazzy",
    "rails-clean-test-db": "RAILS_ENV=test bundle exec rails db:drop; RAILS_ENV=test bundle exec rails db:create; RAILS_ENV=test bundle exec rails db:migrate",
    "rails-server": "rails s",
    "rails-server-test": "RAILS_ENV=test bundle exec rails db:reset; RAILS_ENV=test bundle exec rails s -p 3001 --pid tmp/pids/server_test.pid &",
    "start": "npm-run-all -p -l webpack-hot rails-server",
    "test:all": "jest",
    "test:e2e": "jest --testPathPattern=.*e2e.* --runInBand. --forceExit; kill $(cat tmp/pids/server_test.pid)",
    "test:unit": "jest --testPathPattern=.*.test.js$ --coverage",
    "webpack-hot": "bin/webpack-dev-server --hot"
  },
  "jest": {
    "coverageReporters": [
      "lcov"
    ],
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec|test|e2e).js?(x)"
    ],
    "testURL": "http://localhost/",
    "setupFiles": [
      "./spec/javascript/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "roots": [
      "spec/javascript"
    ],
    "cache": false,
    "resetModules": true,
    "moduleDirectories": [
      "node_modules",
      "app/javascript"
    ]
  },
  "standard": {
    "ignore": [
      "./node_modules/**/*",
      "./spec/javascript/fixtures/*",
      "app/assets/javascripts/cable.js",
      "public/**/*"
    ],
    "env": [
      "jest"
    ],
    "parser": "babel-eslint"
  }
}
